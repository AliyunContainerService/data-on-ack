{
  "name": "ant-design-pro",
  "version": "1.0.0",
  "private": true,
  "description": "An out-of-box UI solution for enterprise applications",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 umi build",
    "build": "cross-env  UMI_ENV=default umi build",
    "deploy": "npm run site && npm run gh-pages",
    "fetch:blocks": "pro fetch-blocks --branch antd@4 && npm run prettier",
    "format-imports": "cross-env import-sort --write '**/*.{js,jsx,ts,tsx}'",
    "gh-pages": "cp CNAME ./dist/ && gh-pages -d dist",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "lint": "npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "check-prettier lint",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "prettier": "prettier -c --write \"**/*\"",
    "start": "cross-env  UMI_ENV=default umi dev",
    "start:no-mock": "cross-env MOCK=none umi dev",
    "start:no-ui": "cross-env UMI_UI=none umi dev",
    "test": "umi test",
    "test:all": "node ./tests/run-tests.js",
    "test:component": "umi test ./src/components",
    "tsc": "tsc",
    "ui": "umi ui",
    "eflops": "cross-env  UMI_ENV=eflops  UMI_UI=none umi dev",
    "eflops-build": "cross-env  UMI_ENV=eflops umi build"
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/icons": "^5.3.1",
    "@ant-design/pro-components": "^2.6.49",
    "@ant-design/pro-form": "1.40.0",
    "@ant-design/pro-layout": "^5.0.19",
    "@ant-design/pro-list": "^2.5.48",
    "@ant-design/pro-table": "^3.14.2",
    "@antv/data-set": "^0.10.2",
    "@npmcli/metavuln-calculator": "^2.0.0",
    "@types/lodash.isequal": "^4.5.8",
    "antd": "^4.24.15",
    "bizcharts": "^4.1.23",
    "classnames": "^2.5.1",
    "echarts": "^5.5.0",
    "lodash": "^4.17.21",
    "lodash-decorators": "^6.0.1",
    "lodash.isequal": "^4.5.0",
    "moment": "^2.30.1",
    "omit.js": "^1.0.2",
    "path-to-regexp": "2.4.0",
    "prop-types": "^15.8.1",
    "qs": "^6.12.0",
    "rc-field-form": "^1.43.0",
    "react": "^17.0.2",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^17.0.2",
    "react-helmet": "^5.2.1",
    "react-lazylog": "^4.5.3",
    "react-monaco-editor": "^0.36.0",
    "terser": "^5.29.1",
    "umi": "^3.5.41",
    "umi-request": "^1.4.0",
    "use-merge-value": "^1.2.0"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "^1.0.28",
    "@types/express": "^4.17.21",
    "@types/history": "^4.7.11",
    "@types/jest": "^24.9.1",
    "@types/lodash": "^4.17.0",
    "@types/qs": "^6.9.12",
    "@types/react": "^16.14.57",
    "@types/react-dom": "^16.9.24",
    "@types/react-helmet": "^5.0.27",
    "@umijs/fabric": "2.0.0",
    "@umijs/plugin-blocks": "^2.2.2",
    "@umijs/preset-ant-design-pro": "^1.3.3",
    "@umijs/preset-react": "^1.8.32",
    "@umijs/preset-ui": "^2.2.9",
    "chalk": "^3.0.0",
    "check-prettier": "^1.0.3",
    "cross-env": "^6.0.3",
    "cross-port-killer": "^1.4.0",
    "echarts-for-react": "^3.0.2",
    "enzyme": "^3.11.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-standard": "^4.1.0",
    "express": "^4.18.3",
    "gh-pages": "^2.2.0",
    "html-webpack-plugin": "^4.5.2",
    "import-sort-cli": "^6.0.0",
    "import-sort-parser-babylon": "^6.0.0",
    "import-sort-parser-typescript": "^6.0.0",
    "import-sort-style-module": "^6.0.0",
    "jest-puppeteer": "^4.4.0",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^9.5.0",
    "mockjs": "^1.1.0",
    "node-fetch": "^2.7.0",
    "prettier": "^3.2.5",
    "pro-download": "1.0.1",
    "stylelint": "^16.2.1",
    "terser-webpack-plugin": "^5.3.10",
    "typescript": "^5.4.2",
    "webpack": "^5.90.3"
  },
  "optionalDependencies": {
    "puppeteer": "^22.4.1"
  },
  "engines": {
    "node": "^16"
  },
  "checkFiles": [
    "src/**/*.js*",
    "src/**/*.ts*",
    "src/**/*.less",
    "config/**/*.js*",
    "scripts/**/*.js"
  ]
}
